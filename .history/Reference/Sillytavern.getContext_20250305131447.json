            <tr>\n<td><h5>Senses:<br></h5></td>\n<td><p>{{character.Senses}}</p></td>\n</tr>\n                    <tr>\n<td><h5>Conditions:<br></h5></td>\n<td><p>{{character.Conditions}}</p></td>\n</tr>\n                    <tr>\n<td><h4>Equipment:<br></h4></td>\n<td>\n    <tr>\n<td><h5>Weapons:<br></h5></td>\n<td><p>{{character.Equipment.Weapons}}</p></td>\n</tr>\n    <tr>\n<td><h5>Armor:<br></h5></td>\n<td><p>{{character.Equipment.Armor}}</p></td>\n</tr>\n    <tr>\n<td><h5>Items:<br></h5></td>\n<td><p>{{character.Equipment.Items}}</p></td>\n</tr>\n</td>\n</tr>\n                    <tr>\n<td><h4>Logs:<br></h4></td>\n<td>\n    <tr>\n<td><h5>Active Missions:<br></h5></td>\n<td><p>{{character.Logs.ActiveMissions}}</p></td>\n</tr>\n    <tr>\n<td><h5>Item Logs:<br></h5></td>\n<td><p>{{character.Logs.ItemLogs}}</p></td>\n</tr>\n    <tr>\n<td><h5>Event Log:<br></h5></td>\n<td><p>{{character.Logs.EventLog}}</p></td>\n</tr>\n    <tr>\n<td><h5>Reputation:<br></h5></td>\n<td><p>{{character.Logs.Reputation}}</p></td>\n</tr>\n</td>\n</tr>\n                </table>\n            </details>\n            {{/foreach}}\n            {{#foreach NonPlayerCharacters character}}\n            <details>\n                <summary><h4>{{character}}</h4></summary>\n                <table>\n                    <tr>\n<td><h5>Hair:<br></h5></td>\n<td><p>{{character.Hair}}</p></td>\n</tr>\n                    <tr>\n<td><h5>Makeup:<br></h5></td>\n<td><p>{{character.Makeup}}</p></td>\n</tr>\n                    <tr>\n<td><h5>Outfit:<br></h5></td>\n<td><p>{{character.Outfit}}</p></td>\n</tr>\n                    <tr>\n<td><h5>State of Dress:<br></h5></td>\n<td><p>{{character.StateOfDress}}</p></td>\n</tr>\n                    <tr>\n<td><h5>Posture and Interaction:<br></h5></td>\n<td><p>{{character.PostureAndInteraction}}</p></td>\n</tr>\n                    {{#if character.PersonalInfo}}\n                    <tr>\n<td><h4>Personal Info:<br></h4></td>\n</tr>\n                    <tr>\n                        <td><h5>Name:<br></h5></td>\n<td><p>{{character.PersonalInfo.Name}}</p></td>\n</tr>\n                    <tr>\n                        <td><h5>Age:<br></h5></td>\n<td><p>{{character.PersonalInfo.Age}}</p></td>\n</tr>\n                    <tr>\n                        <td><h5>Occupation:<br></h5></td>\n<td><p>{{character.PersonalInfo.Occupation}}</p></td>\n</tr>\n                    {{/if}}\n                    {{#if character.PhysicalCharacteristics}}\n                    <tr>\n<td><h4>Physical Characteristics:<br></h4></td>\n</tr>\n                    <tr>\n                        <td><h5>Genetics:<br></h5></td>\n<td><p>{{character.PhysicalCharacteristics.Genetics}}</p></td>\n</tr>\n                    <tr>\n                        <td><h5>Height/Weight:<br></h5></td>\n<td><p>{{character.PhysicalCharacteristics.HeightWeight}}</p></td>\n</tr>\n                    <tr>\n                        <td><h5>Colors:<br></h5></td>\n<td><p>{{character.PhysicalCharacteristics.Colors}}</p></td>\n</tr>\n                    <tr>\n                        <td><h5>Features:<br></h5></td>\n<td><p>{{character.PhysicalCharacteristics.Features}}</p></td>\n</tr>\n                    {{/if}}\n                    {{#if character.MentalPanel}}\n                    <tr>\n<td><h4>Mental Panel:<br></h4></td>\n</tr>\n                    <tr>\n                        <td><h5>Mental State:<br></h5></td>\n<td><p>{{character.MentalPanel.MentalState}}</p></td>\n</tr>\n                    <tr>\n                        <td><h5>Compliance Level:<br></h5></td>\n<td><p>{{character.MentalPanel.ComplianceLevel}}</p></td>\n</tr>\n                    <tr>\n                        <td><h5>Mindbreak:<br></h5></td>\n<td><p>{{character.MentalPanel.Mindbreak}}</p></td>\n</tr>\n                    {{/if}}\n                    {{#if character.SexualInformation}}\n                    <tr>\n<td><h4>Sexual Information:<br></h4></td>\n</tr>\n                    <tr>\n                        <td><h5>Experience:<br></h5></td>\n<td><p>{{character.SexualInformation.Experience}}</p></td>\n</tr>\n                    <tr>\n                        <td><h5>Fetishes:<br></h5></td>\n<td><p>{{character.SexualInformation.Fetishes}}</p></td>\n</tr>\n                    <tr>\n                        <td><h5>Monitor:<br></h5></td>\n<td><p>{{character.SexualInformation.Monitor}}</p></td>\n</tr>\n                    <tr>\n                        <td><h5>SexualPosition:<br></h5></td>\n<td><p>{{character.SexualInformation.SexualPosition}}</p></td>\n</tr>\n                    <tr>\n                        <td><h5>VaginalDepth:<br></h5></td>\n<td><p>{{character.SexualInformation.VaginalDepth}}</p></td>\n</tr>\n                    <tr>\n                        <td><h5>AnalDepth:<br></h5></td>\n<td><p>{{character.SexualInformation.AnalDepth}}</p></td>\n</tr>\n                    <tr>\n                        <td><h5>OralDepth:<br></h5></td>\n<td><p>{{character.SexualInformation.OralDepth}}</p></td>\n</tr>\n                    <tr>\n                        <td><h5>Orgasm:<br></h5></td>\n<td><p>{{character.SexualInformation.Orgasm}}</p></td>\n</tr>\n                    <tr>\n                        <td><h5>LustLevel:<br></h5></td>\n<td><p>{{character.SexualInformation.LustLevel}}</p></td>\n</tr>\n                    {{/if}}\n                </table>\n            </details>\n            {{/foreach}}\n        </div>\n    </details>\n</div>\n<hr>",
