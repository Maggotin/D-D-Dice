                    "content": ":root {\n  --glitched-duration: 0.9s;\n  --glitched-long-duration: 3s;\n\n\n  --green: #11dbab;\n  --light-green: #2cffc7;\n  --transparent-green: rgba(17, 219, 171, 0.21);\n  --dark-green: #0ca984;\n}\n\n* {\n    --stcs--iconSize: calc(var(--nav-bar-width) - 5vh) !important;\n   \n}\n\n\nbody:has(#bg1) {\n\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  scrollbar-color: transparent transparent !important;\n  background-color: var(--SmartThemeBorderColor);\n  display: flex;\n\n  --nav-bar-width: 70px;\n  --avatar-base-width: 50px;\n  --avatar-base-height: 50px;\n  \n  --char-panel-with: 0px;\n  --settings-panel-width: 0px;\n  --authors-note-panel-width: 0px;\n  --expression-image-width: 0px;\n  --cfgConfig-panel-width: 0px;\n  --logprobsViewer-panel-width: 0px;\n  --groupMemberListPopout-panel-width: 0px;\n  --stcdxcodex: 0px;\n  --notebookPanel-width: 0px;\n  --gallery-panel-width: 0px;\n\n\n  --drawer-content-panel-width: calc(100vw - var(--nav-bar-width) - var(--settings-panel-width) - var(--char-panel-width) - var(--authors-note-panel-width) - var(--stcdxcodex) - var(--notebookPanel-width) - var(--groupMemberListPopout-panel-width) - var(--gallery-panel-width) - (var(--theme-margin) * 2) - (var(--theme-margin) / 4));\n  --drawer-content-panel-wit-img-width: calc(100vw - var(--nav-bar-width) - var(--cfgConfig-panel-width) - var(--expression-image-width) - var(--authors-note-panel-width) - var(--char-panel-width) - var(--settings-panel-width) - var(--stcdxcodex) - var(--notebookPanel-width) - var(--groupMemberListPopout-panel-width) - var(--gallery-panel-width) - (var(--theme-margin) * 2) - (var(--theme-margin) / 4));\n  --theme-margin: 10px;\n\n  --elements-height: calc(100svh - (var(--theme-margin) * 2));\n  --box-shadow-color: #00000080;\n  --black50a: color-mix(in srgb, var(--SmartThemeBorderColor) 30%, transparent);\n  --white50a: color-mix(in srgb, var(--SmartThemeUnderlineColor) 60%, rgba(255, 255, 255, 0.5));\n\n  \n}\n\n\n\n\nbody:has(#stcdx--codex.stcdx--active) #WorldInfo {\n  max-width: var(--drawer-content-panel-width);\n}\n\nbody.charListGrid:has(#right-nav-panel.openDrawer):not(#rm_ch_create_block) {\n  --char-panel-width: 620px;\n}\n\n\n\nbody:has(#right-nav-panel.openDrawer) {\n  --char-panel-width: 420px;\n}\n\nbody:has(#rm_ch_create_block:is([style*=\"display: block; opacity: 1;\"])) {\n  --char-panel-width: 420px !important;\n}\n\n\nbody:has(#notebookPanel.flex) {\n  --notebookPanel-width: 400px !important;\n}\n\nbody:has(#notebookPanel.maximized) {\n  --notebookPanel-width: 0px !important;\n}\n\nbody:has(#right-nav-panel.closedDrawer) {\n  --char-panel-width: 0px !important;\n}\n\nbody:has(#left-nav-panel.openDrawer) {\n  --settings-panel-width: 360px !important;\n}\n\n\n\nbody:has(#expression-image:not([src=\"\"])) {\n  --expression-image-width: 512px !important;\n}\n\nbody:has(#expression-image:not([src=\"\"])).waifuMode {\n  --expression-image-width: 0px !important;\n}\n\nbody:has(.zoomed_avatar.draggable) {\n  --expression-image-width: 512px !important;\n}\n\nbody:has(.zoomed_avatar.draggable).waifuMode {\n  --expression-image-width: 0vw !important;\n}\n\nbody:not(:has(.zoomed_avatar.draggable)) {\n  --expression-image-width: 0px;\n}\n\nbody:has(#floatingPrompt:is([style*=\"display: flex; opacity: 1;\"])) {\n  --authors-note-panel-width: 400px;\n}\n\nbody:has(#floatingPrompt.maximized) {\n  --authors-note-panel-width: 0px !important;\n}\n\nbody:has(#cfgConfig:is([style*=\"display: flex; opacity: 1;\"])) {\n  --cfgConfig-panel-width: 360px;\n}\n\nbody:has(#cfgConfig.maximized) {\n  --cfgConfig-panel-width: 0px !important;\n}\n\nbody:has(#groupMemberListPopout) {\n  --groupMemberListPopout-panel-width: 340px;\n}\n\nbody:has(#gallery.draggable) {\n  --gallery-panel-width: 340px;\n}\n\nbody:has(#logprobsViewer:is([style*=\"display: flex; opacity: 1;\"])) {\n  --expression-image-width: 20vw !important;\n}\n\nbody:has(#logprobsViewer.maximized) {\n  --expression-image-width: 0px !important;\n}\n\n#user-settings-block::after {\n  opacity:1;\n}\n#top-bar, #top-settings-holder {\n  margin: unset;\n  width: var(--nav-bar-width);\n  position: fixed;\n  left: 0;\n  opacity: 0;\n  transition: opacity 0.9s ease;\n}\n\n#top-bar {\n  top: 5px;\n  color: transparent;\n  background-color: transparent;\n  height: 56px; /* Adjust as needed */\n  z-index: 1001; /* Ensure it's above #top-settings-holder */\n}\n\n#top-settings-holder {\n  top: 0;\n  height: 90dvh;\n  border: transparent;\n  background-color: transparent;\n  display: flex;\n  flex-direction: column;\n  padding: 16px 8px;\n  gap: 2.22vh;\n  z-index: 1000;\n}\n\n/* Hover effects */\n#top-bar:hover,\n#top-settings-holder:hover,\n#top-bar:hover + #top-settings-holder {\n  opacity: 1;\n}\n\n/* Specific hover effect for #top-settings-holder */\n#top-settings-holder:hover ~ #top-bar {\n  opacity: 1;\n}\n\n/* Rest of your CSS remains the same */\n#top-settings-holder::before {\n  content: \"\";\n  width: 100%;\n  aspect-ratio: 1 / 1;\n  background-image: url(\"img/five.png\");\n  background-size: cover;\n  background-position: center;\n  border-radius: 50%;\n}\n\n\n.drawer {\n  height: 7vh;\n}\n\n.drawer-icon {\n  font-size: 5vh;\n  scale: 0.7;\n  padding: 0 !important; \n  transition: none; \n}\n\n.drawer-icon.closedIcon {\n  opacity: 0.8;\n  transition: none;\n}\n\n#rightNavHolder > .drawer-toggle {\n  width: unset !important;\n}\n\n\n\n/* Drawer Content Styles */\ndiv.drawer-content {\n  backdrop-filter: blur(calc(var(--SmartThemeBlurStrength))) !important;\n  width: var(--drawer-content-panel-width);\n  height: 100dvh;\n  border-radius: unset;\n  margin: 0;\n  max-height: calc(100vh - var(--bottomFormBlockSize));\n  box-shadow: none;\n  background: rgb(39 40 40 / 66%);\n  padding: 16px;\n  position: absolute;\n  left: calc(var(--nav-bar-width) + var(--char-panel-width));\n  right: var(--settings-panel-width);\n  top: 0;\n  z-index: 20 !important;\n}\n\n/* Specific button adjustments */\n#sys-settings-button:not(:has(.online_status_indicator.success)) .drawer-icon,\n#user-settings-button .drawer-icon {\n  margin-left: 4px;\n}\n\n.drawer-icon.openIcon {\n  transition: 0s;\n}\n\n\n\n/* Hover and Focus Styles for Char List and Button */\n#right-nav-panel:has(#rm_PinAndTabs:hover, #rm_PinAndTabs:focus-within) #charListFixedTop,\n#charListFixedTop:hover,\n#charListFixedTop:focus-within {\n  display: block;\n  animation: pop-in var(--animation-duration-slow) ease-in-out;\n}\n\n/* Content Change for Selected Char Button */\n#right-nav-panel:has(#rm_PinAndTabs:hover, #rm_PinAndTabs:focus-within, #charListFixedTop:hover, #charListFixedTop:focus-within) #rm_button_selected_ch h1::before {\n  content: \"\\f139\"; /* Icon content */\n}\n\n\n\n\n/* Character List Grid Styling */\nbody.charListGrid #rm_print_characters_block {\n  flex-direction: column;\n  padding: 5px 8px;\n}\n\n/* Styling for Group and Character Select in Grid */\nbody.charListGrid #rm_print_characters_block .group_select,\nbody.charListGrid #rm_print_characters_block .character_select {\n  width: 100%;\n  flex-direction: row;\n  gap: 0.78vw;\n}\n\n/* Hover Effect */\nbody.charListGrid #rm_print_characters_block .group_select:hover,\nbody.charListGrid #rm_print_characters_block .character_select:hover {\nbackground: linear-gradient(\n    315deg,\n    var(--SmartThemeBorderColor) 0%, \n    var(--SmartThemeBorderColor) 5%, \n    transparent 70%, \n    transparent 98%\n) !important;\n\n}\n\n\n/* Avatar and Character Name Styling */\nbody.charListGrid #rm_print_characters_block .ch_name {\n  text-align: left;\n  font-size: var(--mainFontSize);\n}\n\n\nhtml, body.charListGrid {\n  box-shadow: 0 2px 5px rgba(0, 0, 5);\n  --avatar-width: 6vw;\n  --avatar-aspect-ratio: calc(2 / 3);\n  --avatar-border-width: 1px;\n  --avatar-fav-border-width: 2px;\n  --avatar-padding: 5px;\n}\n\nbody.charListGrid #rm_print_characters_block {\n  display: grid;\n  grid-auto-rows: max-content;\n  grid-template-columns: repeat(\n    auto-fill,\n    minmax(\n      calc(\n        var(--avatar-width) + var(--avatar-border-width) * 2 +\n        var(--avatar-fav-border-width) * 2 + var(--avatar-padding) * 2\n      ),\n      1fr\n    )\n  );\n}\n\nbody.charListGrid #rm_print_characters_block .character_select,\nbody.charListGrid #rm_print_characters_block .group_select {\n  width: 100%;\n  max-height: unset;\n  min-height: unset;\n  max-width: unset;\n  min-width: unset;\n  height: unset;\n}\n\nbody.charListGrid #rm_print_characters_block .avatar.avatar_collage {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(50%, 1fr));\n}\n\nbody.charListGrid #rm_print_characters_block .avatar.avatar_collage img {\n  width: 100%;\n  height: 100%;\n}\n\nbody.charListGrid #rm_print_characters_block .avatar,\nbody.charListGrid #rm_print_characters_block .avatar:not(.avatar_collage) img {\n  width: 100%; /* Full width of the grid cell */\n  height: unset;\n  aspect-ratio: var(--avatar-aspect-ratio);\n  display: block;\n  flex: 0 0 auto;\n}\n\nbody.charListGrid #rm_print_characters_block .character_select_container,\nbody.charListGrid #rm_print_characters_block .group_select_container {\n  align-self: center;\n  max-width: unset;\n}\n\nbody.charListGrid #rm_print_characters_block .ch_name,\nbody.charListGrid #rm_print_characters_block .ch_additional_info {\n  max-width: unset;\n  white-space: unset;\n}\n\n/* Tag Controls Animation */\n#charListFixedTop .rm_tag_controls {\n  display: none;\n}\n\n\n#CharListButtonAndHotSwaps > .flexFlowColumn {\n  flex-direction: column-reverse;\n  margin-left: 6px;\n  margin-top: -1px;\n}\n\n#CharListButtonAndHotSwaps:has(~ #rm_characters_block[style*=\"display: none;\"]) #rm_button_characters {\n  display: unset;\n}\n\n#CharListButtonAndHotSwaps:has(~ #rm_characters_block[style*=\"display: none;\"]) #rm_button_panel_pin_div {\n  display: none;\n}\n\n\n/* Character Select Styling */\n.character_select {\n  border-radius: 4px;\n}\n\n/* Character Block Overflow Control */\n#rm_characters_block {\n  overflow-x: hidden;\n}\n\n/* Character Pagination Grid */\n#rm_print_characters_pagination {\n  display: grid;\n  grid-template-columns: 1fr;\n  position: relative;\n}\n\n/* Pagination Text Styling */\n#rm_print_characters_pagination::after {\n  content: \"Characters\";\n  text-transform: uppercase;\n  font-size: calc(var(--mainFontSize) * 0.8);\n  font-weight: bold;\n  letter-spacing: 0.145ch;\n  padding-left: 13px;\n  display: flex;\n  align-items: flex-end;\n  grid-column: 1 / span 3;\n  grid-row: 4;\n  height: 80%;\n}\n\n/* Character Search Bar Positioning */\n#character_search_bar {\n  position: absolute;\n  top: 1.11vh;  \n  right: 0.625vw; \n  width: 80%;\n}\n\n/* Hide Characters Button */\n#rm_button_characters {\n  display: none;\n  margin-top: 2px;\n}\n\n#rm_button_characters::before {\n  content: \"\\f060\";\n}\n\n/* Pin and Tabs Position Adjustment */\n#rm_PinAndTabs {\n  margin-top: 4px;\n}\n\n#rm_PinAndTabs:has(~ #rm_characters_block[style*=\"display: none;\"]) #rm_button_selected_ch {\n top: 0.83vh;   \n right: 0.625vw; \n}\n\n/* General Avatar Styling */\n.avatar {\nimage-rendering: smooth;\n  width: 2.5vw;  \n  height: 4.44vh; \n}\n\n.avatar img {\n  width: 100%;\n  height: 100%;\n}\n\n/* Collage Avatar Size */\nbody:not(.big-avatars) .avatar_collage {\n  min-width: 2.5vw;\n}\n\n/* Avatar Wrapper with Adjusted Size */\n.mesAvatarWrapper .avatar {\n  width: 3.28vw;  \n  height: 5.83vh;\n}\n\n/* Hidden Zoomed Avatar Image */\n.zoomed_avatar_img {\n  display: none;\n}\n\n/* Hide User Avatars */\n.mes[is_user=\"true\"] .avatar {\n  display: none;\n}\n\n/* Root Avatar Base Size Variables */\n:root {\n--avatar-base-width: 9vw;   \n--avatar-base-height: 9vw;; \n}\n\n\n/* Pagination Styling */\n.paginationjs {\n  grid-row: 3;\n  margin: -4px -3px 8px 8px;\n  flex-direction: row-reverse;\n  justify-content: space-between;\n}\n\n.paginationjs-pages ul li.disabled a,\n.paginationjs-pages ul li a {\n  border: none;\n  opacity: 0.75; /* Applies only to disabled links */\n}\n\n/* Shared Styles for Select, Pagination Items, and Menu Button */\nselect,\n.paginationjs-pages ul li,\n.menu_button {\n  height: 4.03vh;\n  background-color: rgb(30, 31, 34);\n  border-radius: 4px;\n  padding: 4px;\n  border: none;\n  transition: 0s;\n}\n\n/* Pagination Navigation Positioning */\n.paginationjs-nav {\n  position: absolute;\n  bottom: 0;       \n  right: 0.625vw;  \n  padding-bottom: 2px;\n}\n\n/* Bulk Button Common Styling */\n#bulkEditButton,\n#bulkDeleteButton {\n  background: transparent;\n  border: none;\n  margin: 0 8px;\n  padding: 5px;\n  font-size: 1.41vw;\n  width: calc(100% - 8px);\n}\n\n/* Shared After Pseudo-Element Styles */\n#bulkEditButton::after,\n#bulkDeleteButton::after {\n  font-size: calc(var(--mainFontSize) * 0.8);\n  font-family: \"Noto Sans\", \"Noto Color Emoji\", sans-serif;\n  padding-left: 7px;\n  font-weight: bold;\n  letter-spacing: 0.145ch;\n}\n\n/* Content for Each Button */\n#bulkDeleteButton::after {\n  content: \"Bulk Delete\";\n}\n\n#bulkEditButton::after {\n  content: \"Bulk Edit\";\n}\n\n/* Button Positioning in Grid */\n#bulkEditButton {\n  grid-row: 1;\n}\n\n#bulkDeleteButton {\n  grid-row: 2;\n  margin-bottom: 4px !important;\n}\n\n/* Hover and Active States */\n#bulkEditButton:hover,\n#bulkDeleteButton:hover {\n background: linear-gradient(\n    315deg,\n    var(--SmartThemeBorderColor) 0%, \n    var(--SmartThemeBorderColor) 5%, \n    transparent 70%, \n    transparent 98%\n) !important;\n}\n\n#bulkEditButton.bulk_edit_overlay_active {\n  background-color: rgba(78, 80, 88, 0.8);\n}\n\n#bulkDeleteButton:hover {\n  color: rgb(242, 63, 66);\n}\n\n/* Hidden Elements */\nhr,\n#HotSwapWrapper {\n  display: none;\n}\n\n/* Positioning for rm_button_selected_ch */\n#rm_button_selected_ch {\n  position: absolute;\n  right: 10000px;\n  opacity: 1;\n}\n\n\n\n\n\n\n/* Form block buttons */\n.form_create_bottom_buttons_block {\n  width: 70%;\n  margin-left: auto;\n}\n\n/* Result info */\n#result_info {\n  justify-content: flex-end;\n  width: 100%;\n}\n\n\n/* Text area styling */\n.text_pole, textarea, textarea:focus-visible {\n\n\n}\n\n\n/* First message container */\n#first_message_div {\n  flex-direction: column;\n  align-items: flex-start;\n  gap: 6px;\n}\n\n\n#sheld {\n  justify-content: left;\n  padding: 0px;\n  border-radius: 6px;\n  height: var(--elements-height);\n  max-height: unset;\n  top: 0;\n  -webkit-backdrop-filter: blur(var(--SmartThemeBlurStrength));\n  background: transparent;\n}\n/* Shelled background and layout */\n\n\n/* Sheld form styling */\n#mes_block,\n#sheld,\n#chat,\n#bg1,\n#form_sheld {\n  top: 0;\n  padding: 0px;\n  max-height: unset;\n  border: none !important;\n  width: calc(var(--drawer-content-panel-wit-img-width) - (var(--nav-bar-width)));\n  padding-right: var(--nav-bar-width) !important;\n  left: var(--char-panel-width);\n  transition-duration: 0.9s;\n  transition-timing-function: cubic-bezier(0.4, 0, 0.2, 1);\n  transition: opacity 0.9s, transform 0.9s cubic-bezier(0.4, 0, 0.2, 1),\n  width 0.9s cubic-bezier(0.4, 0, 0.2, 1),\n  left 0.9s cubic-bezier(0.4, 0, 0.2, 1),\n  right 0.9s cubic-bezier(0.4, 0, 0.2, 1);\n  \n}\n\n#form_sheld {\n  padding-bottom: 24px;\n  background: none; \n  padding-right: var(--nav-bar-width) !important;\n}\n/* Chat section */\n#chat {\n  max-height: unset;\n  background: transparent;\n  max-width: 200dvh;\n  padding: 24px 6px 12px;\n  backdrop-filter: none;\n}\n\n/* Add smooth transitions */\n/* First, define our keyframe animations */\n@keyframes slideInFromLeft {\n  from {\n      transform: translateX(-100%);\n      opacity: 0;\n  }\n  to {\n      transform: translateX(0);\n      opacity: 0.9;\n  }\n}\n\n@keyframes slideInFromRight {\n  from {\n      transform: translateX(100%);\n      opacity: 0;\n  }\n  to {\n      transform: translateX(0);\n      opacity: 0.9;\n  }\n}\n\n@keyframes slideOutToLeft {\n  from {\n      transform: translateX(0);\n      opacity: 0.9;\n  }\n  to {\n      transform: translateX(-100%);\n      opacity: 0;\n  }\n}\n\n@keyframes slideOutToRight {\n  from {\n      transform: translateX(0);\n      opacity: 0.9;\n  }\n  to {\n      transform: translateX(100%);\n      opacity: 0;\n  }\n}\n/* Navigation Panel Styles */\n#right-nav-panel,\n#left-nav-panel {\n  position: fixed;\n  top: 0;\n  height: 100dvh;\n  z-index: 10 !important;\n  max-height: unset;\n  border-radius: unset;\n  border: unset;\n  min-width: unset;\n  background-color: var(--SmartThemeBlurTintColor);\n  backdrop-filter: blur(calc(var(--SmartThemeBlurStrength))) !important; /* Add these new properties */\n    transition: transform 0.9s cubic-bezier(0.4, 0, 0.2, 1),\n  width 0.9s cubic-bezier(0.4, 0, 0.2, 1),\n  left 0.9s cubic-bezier(0.4, 0, 0.2, 1),\n  right 0.9s cubic-bezier(0.4, 0, 0.2, 1);\n  width: 1px;\n  opacity: 1;\n}\n\n#right-nav-panel {\n  left: var(--nav-bar-width);\n  width: var(--char-panel-width);\n  /* Add this new property */\n\n}\n\n#left-nav-panel {\n  right: 0;\n\n  /* Add this new property */\n}\n/* Right panel animations */\n#right-nav-panel.openDrawer {\n  animation: slideInFromLeft 0.9s ease-in-out (0.4, 0, 0.2, 1) forwards;\n}\n\n#right-nav-panel.closedDrawer {\n  animation: slideOutToLeft 0.9s ease-in-out (0.4, 0, 0.2, 1) forwards;\n}\n\n/* Left panel animations */\n#left-nav-panel.openDrawer {\n  animation: slideInFromRight 0.9s ease-in-out (0.4, 0, 0.2, 1) forwards;\n}\n\n#left-nav-panel.closedDrawer {\n  animation: slideOutToRight 0.9s ease-in-out (0.4, 0, 0.2, 1) forwards;\n}\n\n\n\n#left-nav-panel.drawer-content {\n  margin: var(--theme-margin) !important;\n  right: -9px;\n  padding: 0;\n  left: unset;\n  width: calc(var(--settings-panel-width) - (var(--theme-margin) / 4));\n  border: unset;\n  height: var(--elements-height);\n  max-height: unset;\n}\n\n\n\n\n\n#clickSlidersTips {\n  display: none !important;\n}\n\n\n\n\n/* Margin adjustment for pin button */\n#lm_button_panel_pin_div {\n  margin-left: 8px;\n}\n\n/* Positioning for the help button */\n.note-link-span.topRightInset[name=\"samplerHelpButton\"] {\n  top: 1.11vh;   \n  right: 0.47vw; \n}\n\n/* Styling for neo-range input */\n.neo-range-input {\n  border: none;\n  background: rgb(30, 31, 34);\n  margin: 8px 0;\n  padding: 4px;\n  border-radius: 4px;\n}\n\n/* Neo range slider */\n.neo-range-slider, input[type=\"range\"] {\n  background: rgba(255, 255, 255, 0.6)!important;\n  border-radius: 4px !important;\n  height: 0.625vw !important;\n  -webkit-appearance: none;\n  -moz-appearance: none;\n}\n\n/* Range thumb styling for webkit and moz browsers */\ninput[type=\"range\"]::-webkit-slider-thumb,\ninput[type=\"range\"]::-moz-range-thumb {\n  -webkit-appearance: none;\n  width: 0.47vw;  \n  height: 2.5vh;  \n  border-radius: 3px;\n  border: 1.5px solid gray;\n  box-shadow: none;\n  box-sizing: content-box;\n}\n\n/* Number input styling */\ninput[type=\"number\"] {\n  -moz-appearance: textfield;\n  border: none;\n  border-radius: 4px;\n  height: 4.03vh;\n}\n\ninput[type=\"number\"]::-webkit-inner-spin-button,\ninput[type=\"number\"]::-webkit-outer-spin-button {\n  -webkit-appearance: none;\n}\n\n/* Checkbox styling */\ninput[type=\"checkbox\"] {\n  margin: 0;\n  padding: 0;\n  border: none !important;\n  background: grey!important;\n  border-radius: 100% !important;\n  outline: none !important;\n  box-shadow: none !important;\n}\n\ninput[type=\"checkbox\"]:checked {\n  background: #22a559 !important;\n}\n\ninput[type=\"checkbox\"]:checked::before {\n  content: \"\\f00c\" !important;\n  font-family: \"Font Awesome 6 Free\";\n  background: #22a559 !important;\n  color: white !important;\n  font-size: 0.6em;\n  font-weight: bold;\n  transition: none !important;\n}\n\n\n/* Centering alignment for checkbox labels and settings blocks */\n#anchor_checkbox label,\n#power-user-option-checkboxes label,\n.checkbox_label,\n.auto_continue_settings_block {\n  display: flex;\n  align-items: center;\n}\n\n\n/* Message container styling with hover effect */\n\n\n.mes:hover {\n  background: rgba(2, 2, 2, 0.06);\n}\n\n.mes:hover .mes_buttons {\n  display: flex;\n}\n\n/* Mes buttons styling */\n.mes_buttons {\n    display: none;\n    position: absolute;\n    top: -0.1vh;\n    right: 0.1vw;\n    padding: 4px;\n    border-radius: 3px;\n}\n\n/* Styling for mes buttons and extra elements */\n.mes_buttons .mes_edit,\n.mes_buttons .mes_bookmark,\n.mes_buttons .mes_create_bookmark,\n.extraMesButtonsHint,\n.tagListHint,\n.extraMesButtons div {\n  color: rgba(181, 186, 193, 0.59) !important;  \n  opacity: 1 !important;\n}\n\n\n/* General styling for settings headers and toggles */\n#extensions_settings .inline-drawer-toggle.inline-drawer-header,\n#extensions_settings2 .inline-drawer-toggle.inline-drawer-header,\n#user-settings-block h4,\n.standoutHeader {\n  background: transparent;\n  border: none;\n  font-size: 1.2em;\n}\n\n/* Keyframes for sliding down and up */\n@keyframes slideDown {\n  from {\n    transform: translateY(-20px);\n    opacity: 0;\n  }\n  to {\n    transform: translateY(0);\n    opacity: 1;\n  }\n}\n\n@keyframes slideUp {\n  from {\n    transform: translateY(0);\n    opacity: 1;\n  }\n  to {\n    transform: translateY(-20px);\n    opacity: 0;\n  }\n}\n\n/* Apply to all relevant windows */\n#user-settings-block,\n#api-connections-block,\n#ai-response-formatting-block,\n#change-background-block,\n.drawer-content {\n  animation: slideDown 0.9s ease-out forwards;\n  animation-fill-mode: both;\n}\n\n/* When closing, add a class 'closing' to trigger slide up */\n\n/* Extension top bar styling */\n#extensionTopBar {\n  display: flex;\n  margin-bottom: 1.39vh;\n  flex-direction: row;\n  flex-wrap: wrap;\n  align-items: baseline;\n  align-content: center;\n  width: 100%;\n  margin-left: 0vw;\n  border: 2px solid var(--SmartThemeBorderColor);\n  border-radius: 20px !important;\n  background-color: rgba(0, 0, 0, 0.81); /* Transparent background with opacity */\n  backdrop-filter: blur(var(--SmartThemeBlurStrength));\n  height: var(--bottomFormBlockSize);\n  padding: 4px;\n  gap: 0.75vw;  \n  margin: 0.3vw, 0.69vh; \n  font-size: calc(var(--mainFontSize) * 0.9);\n  opacity: 0; /* Initial opacity for transition */\n  transition: opacity 0.4s ease; /* Transition applied only to opacity */\n}\n\n/* Hover effect for extension top bar */\n#extensionTopBar:hover {\n  opacity: 1;\n}\n\n/* Hide online status indicator and drag grabber */\n.online_status,\n.drag-grabber {\n  display: none !important;\n}\n\n\n\n/* Set height for text pole */\n.text_pole {\n  height: 4.03vh;\n}\n\n\n/* Styling for world info container list items */\n#select2-world_info-container > li {\n  padding: 2px;\n}\n\n#select2-world_info-container > li > button {\n  height: 100%;\n  width: 1.25vw; \n}\n\n#select2-world_info-container > li > button span {\n  height: 1.39vh;  \n  width: 0.78vw; \n  line-height: 8px;\n  display: block;\n}\n\n#select2-world_info-container > li > span {\n  margin-left: 0.94vw;\n}\n\n/* Set width to default for bg_list */\n.bg_list {\n  width: unset;\n}\n\n/* Font Awesome icon styling for h1 in rm_button_selected_ch */\n#rm_button_selected_ch h1::before {\n  vertical-align: middle;\n  text-align: center;\n  content: \"\\f13a\";\n  margin-right: 0.47vw;;\n  line-height: 1; \n  font-family: \"Font Awesome 6 Free\";\n}\n\n/* Remove default bubble background styles */\nbody.bubblechat .mes {\n  padding: 5px;\n  border-radius: unset;\n  background-color: unset !important;\n  margin-bottom: 0;\n  border: none;\n}\n\nbody.bubblechat .mes[is_user=\"true\"] {\n  background-color: unset !important;\n}\n\n/* Modify message block appearance for bot and user */\n/* Modify message block appearance for bot and user */\nbody.bubblechat .mes_block {\n  background-color: rgba(0, 0, 0, 0.6); /* Darkened background */\n  padding: 10px;\n  border-radius: 10px;\n  border: 1px solid var(--SmartThemeBorderColor);\n  margin-left: 0.39vw; \n  width: max-content;\n  padding-bottom: 0;\n}\n\nbody.bubblechat .mes[is_user=\"true\"] .mes_block {\n  background-color: rgba(0, 0, 0, 0.7); /* Slightly darker for user messages */\n  order: -1;\n  margin-left: auto;\n  margin-right: 0;\n}\n\n/* Hide user name/date line in message block */\n.mes[is_user=\"true\"] .mes_block  .ch_name .name_text {\n  display: none; \n}\n\n/* Hide name/date line in message block */\n .mes_block  .ch_name  {\n  display: inline; \n}\n\n/* QR Bar Transition on Send Form Hover */\n#send_form:hover #qr--bar {\n  height: 5.56vh; \n  transition: height 0.5s ease; \n  background: transparent;\n}\n\n/* QR Bar Base Styling */\n#qr--bar { \n  border-top: 4px transparent var(--SmartThemeQuoteColor);\n  height: 0.56vh;\n  background: transparent;\n}\n\n/* Order Settings for UI Elements */\n#logo_block,\n#persona-management-button,\n#extensions-settings-button,\n#advanced-formatting-button {\n  order: 9;\n}\n\n#user-settings-button {\n  order: 3;\n}\n\n#sys-settings-button {\n  order: 4;\n}\n\n#WI-SP-button {\n  order: 10;\n}\n\n#rightNavHolder {\n  order: 0;\n}\n\n\n/* Hide Webkit Scrollbars */\n::-webkit-scrollbar {\n  width: 0;\n  height: 0;\n  scrollbar-gutter: stable;\n}\n\n/* Edit Textarea Styling */\n.edit_textarea {\n  font-family: 'Cascadia Code';\n  height: 69.44vh;   \n  width: 39vw;    \n}\n\n/* Saturation Filter on HTML */\nhtml {\n  filter: saturate(1.5);\n}\n\n/* Message Container Hidden Elements and Hover Visibility Transition */\n.mes .mfc--root {\n  visibility: hidden;\n  opacity: 0;\n  transition: visibility 0s 0.5s, opacity 0.5s linear;\n}\n\n.mes:hover .mfc--root {\n  visibility: visible;\n  opacity: 0.4;\n  transition: opacity 0.1s linear;\n}\n\n/* Send Form Styling */\n#send_form {\n  border-radius: 20px;\n  padding: 8px 16px;\n  box-shadow: 0px 2px 5px rgba(0, 0, 5)!important;\n  border: 1px solid var(--SmartThemeBorderColor) !important;\n}\n\n#send_form, #qr--bar {\n  opacity: 0.5; /* Adjust this value between 0 and 1 */\n}\n\n#send_form:ho