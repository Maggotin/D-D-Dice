                    "code": "-- Extension registration\nEXTENSION_NAME = \"MessageTypewriter\"\n\n-- Configuration\nlocal CHAR_TIME = 30\nlocal state = { active = false }\n\n-- Helper function to apply typewriter effect to a message\nfunction animateMessage(messageElement)\n    local text = messageElement:getText()\n    messageElement:setText(\"\")\n    \n    local index = 1\n    local function addChar()\n        if index <= #text then\n            local char = document.createElement(\"span\")\n            char:setAttribute(\"class\", \"char\")\n            char:setText(\"▌\")\n            messageElement:appendChild(char)\n            \n            -- Animate the character\n            setTimeout(function()\n                char:setText(text:sub(index, index))\n                char:addClass(\"fade-in\")\n            end, CHAR_TIME)\n            \n            index = index + 1\n            setTimeout(addChar, CHAR_TIME)\n        end\n    end\n    \n    setTimeout(addChar, 1000)\nend\n\n-- Extension initialization\nfunction startup()\n    -- Register extension\n    registerExtension({\n        name = EXTENSION_NAME,\n        version = \"1.0\",\n        author = \"YourName\",\n        description = \"Adds typewriter effect to messages\",\n    })\n    \n    -- Add required CSS\n    local style = [[\n        .mes_text .char {\n            opacity: 0;\n            display: inline-block;\n            transition: opacity 0.3s ease;\n        }\n        \n        .mes_text .char.fade-in {\n            opacity: 1;\n            text-shadow: 0px 0px 4px var(--green);\n            animation: textGlow 1s ease-out forwards;\n        }\n        \n        @keyframes textGlow {\n            0% {\n                color: #fff;\n                text-shadow: 0 0 4rem #fff;\n            }\n            100% {\n                color: var(--green);\n                text-shadow: 0 0 1rem var(--green),\n                             0 0 2rem var(--green),\n                             0 0 3rem v