                    "content": "#chat > .mes:has(~ .lastInContext) { opacity: 0.5; }\n\n#qr--bar > .qr--buttons .qr--button.qr--hasCtx > .qr--button-expander,\n#qr--popout > .qr--body > .qr--buttons .qr--button.qr--hasCtx > .qr--button-expander\n{\n    display: none;\n}\n\nbody #chat .mes_text pre code {\nfont-family: 'Imprint MT Shadow';\ncolor: grey !important;\nline-height: 1.1;\n\n}\n\nscript {\n  display: none !important;\n}\n\n\n\n.tag {\n border: none !important;\n}\n\n.mes .mes_buttons {\n  visibility: hidden;\n  opacity: 0;\n}\n.mes:hover .mes_buttons {\n  visibility: visible;\n  opacity: 1;\n}\n\n/* Swipes Counter Text Color */\n.swipes-counter {\n  color: var(--SmartThemeQuoteColor);\n}\n\n\n\n/* Send Message Background and Text Color */\n#send_form {\n  color: var(--SmartThemeQuoteColor);\n}\n\n/* Chat Character and User Name Color */\n.name_text {\n  color: var(--SmartThemeQuoteColor);\n}\n\nbody.big-avatars .avatar {\n    align-items: unset;\n}\n\n.character_select .avatar,\n.group_member .avatar,\n#group_avatar_preview .avatar {\n    --big-avatar-height-factor: 1.8 !important;\n    --big-avatar-width-factor: 1.2 !important;\n    --big-avatar-border-factor: 5 !important;\n}\n\nbody.charListGrid .flexFlowColumn .avatar:not(.add_avatar):not(.avatar_collage) {\n    outline: 1px solid var(--SmartThemeQuoteColor);\n    box-shadow: 1px 3px 5px var(--SmartThemeQuoteColor);\n    --big-avatar-height-factor: 3 !important;\n    --big-avatar-width-factor: 2 !important;\n    --big-avatar-border-factor: 2 !important;\n}\n\n\n.character_select.is_fav .avatar, .group_select.is_fav .avatar, .group_member.is_fav .avatar, .avatar.is_fav {\n    outline: 1px solid var(--SmartThemeUnderlineColor) !important;\n    box-shadow: 1px 1px 5px var(--SmartThemeUnderlineColor) !important;\n}\n\n.character_select.is_fav .ch_name, .group_select.is_fav .ch_name, .group_member.is_fav .ch_name {\n    color: var(--SmartThemeUnderlineColor);\n}",
