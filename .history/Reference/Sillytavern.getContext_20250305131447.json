                    "content": "<style>\n@import url('https://fonts.googleapis.com/css2?family=Roboto+Condensed:wght@400;700&display=swap');\n\nbody {\n    background-color: #282c34;\n    color: #FFFFFF;\n}\n\n.character-sheet {\n    font-family: 'Roboto Condensed', sans-serif;\n    padding: 20px;\n    max-width: 1200px;\n    margin: 0 auto;\n}\n\n.section {\n    background: rgba(0, 0, 0, 0.3);\n    border: 1.9px solid #C53131;\n    border-radius: 3px;\n    padding: 15px;\n    margin-bottom: 20px;\n}\n\n\n\n\n.section-title {\n    color: #FFB158;\n    font-weight: bold;\n    text-transform: uppercase;\n    margin-bottom: 10px;\n    padding-bottom: 5px;\n    font-size: 14px;\n}\n\n.grid-container {\n    display: grid;\n    grid-template-columns: repeat(3, 1fr);\n    gap: 10px;\n}\n\n.grid-container-bg {\n    background-image: url('saving-throws-box.svg') !important;\n    background-size: 100% 100%;\n    background-repeat: no-repeat;\n    position: relative;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n   \n}\n\n.stat-block {\n    background: rgba(0, 0, 0, 0.3);\n    border: 1.9px solid #C53131;\n    border-radius: 3px;\n    padding: 10px;\n    text-align: center;\n    color: #FFB158;\n    font-size: 14px;\n}\n\n.content-list {\n    color: #FFFFFF;\n    padding: 5px;\n    font-size: 14px;\n}\n\n.feature-list {\n    list-style-type: none;\n    padding-left: 0;\n}\n\n.feature-list li {\n    color: #FFB158;\n    padding: 5px;\n    margin-bottom: 5px;\n    font-size: 14px;\n    border: 1.9px solid #C53131;\n    border-radius: 3px;\n    background: rgba(0, 0, 0, 0.3);\n}\n\n.spell-level-title {\n    color: #FFB158;\n    font-weight: bold;\n    text-transform: uppercase;\n    margin-bottom: 5px;\n    font-size: 14px;\n}\n\n.spell-list div {\n    color: #FFFFFF;\n    padding: 5px;\n    margin-bottom: 5px;\n    font-size: 14px;\n    border: 1.9px solid #C53131;\n    border-radius: 3px;\n    background: rgba(0, 0, 0, 0.3);\n}\n\n.skills-section {\n    width: 100%;\n}\n\n.skills-content {\n    color: #FFB158;\n    font-size: 14px;\n    line-height: 1.4;\n    padding: 10px;\n    text-align: left;\n    word-wrap: break-word;\n    display: flex;\n    flex-wrap: wrap;\n    gap: 10px;\n}\n\n.skill-item {\n    white-space: nowrap;\n    background: rgba(0, 0, 0, 0.3);\n    border: 1.9px solid #C53131;\n    border-radius: 3px;\n    padding: 5px 10px;\n}\n\n.sheet-header {\n    text-align: center;\n    padding-bottom: 10px;\n    margin-bottom: 15px;\n    color: #FFB158;\n    border-bottom: 1.9px solid #C53131;\n}\n\n.sheet-header h1 {\n    font-size: 24px;\n    text-transform: uppercase;\n    margin: 0;\n}\n\n@media (max-width: 768px) {\n    .grid-container {\n        grid-template-columns: 1fr;\n    }\n}\n</style>\n\n<div class=\"character-sheet\">\n    <div class=\"sheet-header\">\n        <h1>Character Sheet: Iskandar</h1>\n    </div>\n\n    <div class=\"section\">\n        <div class=\"section-title\">Basic Information</div>\n        <div class=\"grid-container\">\n            <div class=\"grid-container-bg\">\n            <div class=\"stat-block\">Name: Iskandar</div>\n            <div class=\"stat-block\">Race: [Race]</div>\n            <div class=\"stat-block\">Class: [Class]</div>\n            <div class=\"stat-block\">Level: [Level]</div>\n            <div class=\"stat-block\">XP: [Current XP]/[Next Level XP]</div>\n        </div>\n    </div>\n</div>\n    <div class=\"section\">\n        <div class=\"section-title\">Attributes</div>\n        <div class=\"grid-container\">\n            <div class=\"stat-block\">Strength: [Score] (+[Modifier])</div>\n            <div class=\"stat-block\">Dexterity: [Score] (+[Modifier])</div>\n            <div class=\"stat-block\">Constitution: [Score] (+[Modifier])</div>\n            <div class=\"stat-block\">Intelligence: [Score] (+[Modifier])</div>\n            <div class=\"stat-block\">Wisdom: [Score] (+[Modifier])</div>\n            <div class=\"stat-block\">Charisma: [Score] (+[Modifier])</div>\n        </div>\n    </div>\n\n    <div class=\"section\">\n        <div class=\"section-title\">Saving Throws</div>\n        <div class=\"grid-container\">\n            <div class=\"stat-block\">Strength: +[Total] ([Bonus])</div>\n            <div class=\"stat-block\">Dexterity: +[Total] ([Bonus])</div>\n            <div class=\"stat-block\">Constitution: +[Total] ([Bonus])</div>\n            <div class=\"stat-block\">Intelligence: +[Total] ([Bonus])</div>\n            <div class=\"stat-block\">Wisdom: +[Total] ([Bonus])</div>\n            <div class=\"stat-block\">Charisma: +[Total] ([Bonus])</div>\n        </div>\n    </div>\n\n    <div class=\"section\">\n        <div class=\"section-title\">Health and Defense</div>\n        <div class=\"grid-container\">\n            <div class=\"stat-block\">HP: [Current]/[Total]</div>\n            <div class=\"stat-block\">Hit Die: [Current]/[Total] ([Die Type])</div>\n            <div class=\"stat-block\">Temporary HP: [Value]</div>\n            <div class=\"stat-block\">Armor Class: [Value]</div>\n            <div class=\"stat-block\">Initiative: +[Value]</div>\n        </div>\n    </div>\n\n    <div class=\"section\">\n        <div class=\"section-title\">Skills</div>\n        <div class=\"skills-content\">\n            <span class=\"skill-item\">Animal Handling: (+[Modifier])</span>\n            <span class=\"skill-item\">Arcana: (+[Modifier])</span>\n            <span class=\"skill-item\">Athletics: (+[Modifier])</span>\n            <span class=\"skill-item\">Deception: (+[Modifier])</span>\n            <span class=\"skill-item\">History: (+[Modifier])</span>\n            <span class=\"skill-item\">Insight: (+[Modifier])</span>\n            <span class=\"skill-item\">Intimidation: (+[Modifier])</span>\n            <span class=\"skill-item\">Investigation: (+[Modifier])</span>\n            <span class=\"skill-item\">Medicine: (+[Modifier])</span>\n            <span class=\"skill-item\">Nature: (+[Modifier])</span>\n            <span class=\"skill-item\">Perception: (+[Modifier])</span>\n            <span class=\"skill-item\">Performance: (+[Modifier])</span>\n            <span class=\"skill-item\">Persuasion: (+[Modifier])</span>\n            <span class=\"skill-item\">Religion: (+[Modifier])</span>\n            <span class=\"skill-item\">Sleight of Hand: (+[Modifier])</span>\n            <span class=\"skill-item\">Stealth: (+[Modifier])</span>\n            <span class=\"skill-item\">Survival: (+[Modifier])</span>\n        </div>\n    </div>\n\n    <div class=\"section\">\n        <div class=\"section-title\">Spellcasting</div>\n        <div class=\"grid-container\">\n            <div class=\"stat-block\">Spell Save DC: [Value]</div>\n            <div class=\"stat-block\">Spell Attack Modifier: +[Value]</div>\n            <div class=\"stat-block\">AEP: [Current]/[Total]</div>\n        </div>\n    </div>\n    \n    <div class=\"section\">\n        <div class=\"section-title\">Spell Slots</div>\n        <div class=\"spell-list\">\n            <div>1st Level: [Current]/[Total]</div>\n            <div>2nd Level: [Current]/[Total]</div>\n            <div>3rd Level: [Current]/[Total]</div>\n        </div>\n    </div>\n\n    <div class=\"section\">\n        <div class=\"section-title\">Spells Known</div>\n        \n        <div class=\"spell-level-group\">\n            <div class=\"spell-level-title\">Cantrips</div>\n            <div class=\"spell-list\">\n                <div>[Cantrip]</div>\n                <div>[Cantrip]</div>\n                <div>[Cantrip]</div>\n            </div>\n        </div>\n        \n        <div class=\"spell-level-group\">\n            <div class=\"spell-level-title\">1st Level Spells</div>\n            <div class=\"spell-list\">\n                <div>[Spell]</div>\n                <div>[Spell]</div>\n                <div>[Spell]</div>\n            </div>\n        </div>\n        \n        <div class=\"spell-level-group\">\n            <div class=\"spell-level-title\">2nd Level Spells</div>\n            <div class=\"spell-list\">\n                <div>[Spell]</div>\n                <div>[Spell]</div>\n                <div>[Spell]</div>\n            </div>\n        </div>\n    </div>\n\n    <div class=\"section\">\n        <div class=\"section-title\">Proficiencies</div>\n        <div class=\"grid-container\">\n            <div class=\"stat-block\">\n                <strong>Armor:</strong> \n                <div class=\"content-list\">[Armor], [Armor], [Armor]</div>\n            </div>\n            <div class=\"stat-block\">\n                <strong>Weapons:</strong>\n                <div class=\"content-list\">[Weapon], [Weapon], [Weapon]</div>\n            </div>\n            <div class=\"stat-block\">\n                <strong>Tools:</strong>\n                <div class=\"content-list\">[Tool]: [Ability] (+[Modifier])</div>\n            </div>\n        </div>\n    </div>\n\n    <div class=\"section\">\n        <div class=\"section-title\">Equipped Items</div>\n        <div class=\"grid-container\">\n            <div class=\"stat-block\">\n                <strong>Armor:</strong>\n                <div class=\"content-list\">[Armor]</div>\n            </div>\n            <div class=\"stat-block\">\n                <strong>Weapons:</strong>\n                <div class=\"content-list\">[Weapon], [Weapon], [Weapon]</div>\n            </div>\n            <div class=\"stat-block\">\n                <strong>Other Equipment:</strong>\n                <div class=\"content-list\">[Other], [Other], [Other]</div>\n            </div>\n        </div>\n    </div>\n\n    <div class=\"section\">\n        <div class=\"section-title\">Racial Traits</div>\n        <ul class=\"feature-list\">\n            <li>[Trait]</li>\n            <li>[Trait]</li>\n            <li>[Trait]</li>\n        </ul>\n    </div>\n\n    <div class=\"section\">\n        <div class=\"section-title\">Class Features</div>\n        <ul class=\"feature-list\">\n            <li>[Class Feature]</li>\n            <li>[Class Feature]</li>\n            <li>[Class Feature]</li>\n        </ul>\n    </div>\n\n    <div class=\"section\">\n        <div class=\"section-title\">Implants</div>\n        <ul class=\"feature-list\">\n            <li>[Implant]</li>\n            <li>[Implant]</li>\n            <li>[Implant]</li>\n        </ul>\n    </div>\n\n    <div class=\"section\">\n        <div class=\"section-title\">Feats</div>\n        <ul class=\"feature-list\">\n            <li>[Feat]</li>\n            <li>[Feat]</li>\n            <li>[Feat]</li>\n        </ul>\n    </div>\n\n    <div class=\"section\">\n        <div class=\"section-title\">Resources</div>\n        <ul class=\"feature-list\">\n            <li>[Resource]: [Current]/[Total] ([Die Type])</li>\n            <li>[Resource]: [Current]/[Total] ([Die Type])</li>\n        </ul>\n    </div>\n</div>",
