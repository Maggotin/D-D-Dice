                    "content": "#chat {\n    position: relative;\n    background: linear-gradient(to bottom, rgba(0, 255, 0, 0.1), rgba(0, 255, 0, 0.3), rgba(0, 255, 0, 0.1));\n    color: #0f0;\n    padding: 20px;\n    border-radius: 5px;\n    box-shadow: 0 0 10px rgba(0, 255, 0, 0.5), 0 0 20px rgba(0, 255, 0, 0.5), 0 0 30px rgba(0, 255, 0, 0.5);\n  \n  }\n  \n\n  \n  @keyframes scanline {\n    0% {\n      background-position: 0 0;\n    }\n    100% {\n      background-position: 0 100%;\n    }\n  }\n  \n  \n  /* Terminal Look */\n  body, .send_textarea {\n      color: #00FF00 !important; /* Green text like SCP Terminal */\n      background-color: #000000 !important; /* Dark background for a terminal effect */\n      overflow: hidden;\n      position: relative;\n  }\n  \n  /* Messages and Text Styling */\n  .mes_text, .mes_text *, .name_text, .timestamp, .mes_timer, .tokenCounterDisplay {\n      color: #00FF00 !important;  /* Green text for messages */\n      animation: textShadow 1.6s infinite;  /* Apply text shadow animation */\n  }\n  \n  /* Flickering CRT Effect */\n  @keyframes flicker {\n    0% {\n      opacity: 0.27861;\n    }\n    5% {\n      opacity: 0.34769;\n    }\n    10% {\n      opacity: 0.23604;\n    }\n    15% {\n      opacity: 0.90626;\n    }\n    20% {\n      opacity: 0.18128;\n    }\n    25% {\n      opacity: 0.83891;\n    }\n    30% {\n      opacity: 0.65583;\n    }\n    35% {\n      opacity: 0.67807;\n    }\n    40% {\n      opacity: 0.26559;\n    }\n    45% {\n      opacity: 0.84693;\n    }\n    50% {\n      opacity: 0.96019;\n    }\n    55% {\n      opacity: 0.08594;\n    }\n    60% {\n      opacity: 0.20313;\n    }\n    65% {\n      opacity: 0.71988;\n    }\n    70% {\n      opacity: 0.53455;\n    }\n    75% {\n      opacity: 0.37288;\n    }\n    80% {\n      opacity: 0.71428;\n    }\n    85% {\n      opacity: 0.70419;\n    }\n    90% {\n      opacity: 0.7003;\n    }\n    95% {\n      opacity: 0.36108;\n    }\n    100% {\n      opacity: 0.24387;\n    }\n  }\n  \n  /* Text Shadow Animation for CRT Glowing */\n  @keyframes textShadow {\n    0% {\n      text-shadow: 0.4389924193300864px 0 1px rgba(0,30,255,0.5), -0.4389924193300864px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    5% {\n      text-shadow: 2.7928974010788217px 0 1px rgba(0,30,255,0.5), -2.7928974010788217px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    10% {\n      text-shadow: 0.02956275843481219px 0 1px rgba(0,30,255,0.5), -0.02956275843481219px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    15% {\n      text-shadow: 0.40218538552878136px 0 1px rgba(0,30,255,0.5), -0.40218538552878136px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    20% {\n      text-shadow: 3.4794037899852017px 0 1px rgba(0,30,255,0.5), -3.4794037899852017px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    25% {\n      text-shadow: 1.6125630401149584px 0 1px rgba(0,30,255,0.5), -1.6125630401149584px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    30% {\n      text-shadow: 0.7015590085143956px 0 1px rgba(0,30,255,0.5), -0.7015590085143956px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    35% {\n      text-shadow: 3.896914047650351px 0 1px rgba(0,30,255,0.5), -3.896914047650351px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    40% {\n      text-shadow: 3.870905614848819px 0 1px rgba(0,30,255,0.5), -3.870905614848819px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    45% {\n      text-shadow: 2.231056963361899px 0 1px rgba(0,30,255,0.5), -2.231056963361899px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    50% {\n      text-shadow: 0.08084290417898504px 0 1px rgba(0,30,255,0.5), -0.08084290417898504px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    55% {\n      text-shadow: 2.3758461067427543px 0 1px rgba(0,30,255,0.5), -2.3758461067427543px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    60% {\n      text-shadow: 2.202193051050636px 0 1px rgba(0,30,255,0.5), -2.202193051050636px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    65% {\n      text-shadow: 2.8638780614874975px 0 1px rgba(0,30,255,0.5), -2.8638780614874975px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    70% {\n      text-shadow: 0.48874025155497314px 0 1px rgba(0,30,255,0.5), -0.48874025155497314px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    75% {\n      text-shadow: 1.8948491305757957px 0 1px rgba(0,30,255,0.5), -1.8948491305757957px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    80% {\n      text-shadow: 0.0833037308038857px 0 1px rgba(0,30,255,0.5), -0.0833037308038857px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    85% {\n      text-shadow: 0.09769827255241735px 0 1px rgba(0,30,255,0.5), -0.09769827255241735px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    90% {\n      text-shadow: 3.443339761481782px 0 1px rgba(0,30,255,0.5), -3.443339761481782px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    95% {\n      text-shadow: 2.1841838852799786px 0 1px rgba(0,30,255,0.5), -2.1841838852799786px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }\n    100% {\n      text-shadow: 2.1841838852799786px 0 1px rgba(0,30,255,0.5), -2.1841838852799786px 0 1px rgba(255,0,80,0.3), 0 0 3px;\n    }}",
